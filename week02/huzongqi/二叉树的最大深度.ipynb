   "source": [
    "class Solution():\n",
    "    def maxDepth(self, root):\n",
    "        if root is None:\n",
    "            return 0\n",
    "        else:\n",
    "            left_height = self.maxDepth(root.left)\n",
    "            right_heught = self.maxDepth(root.right)\n",
    "\n",
    "        return max(left_height, right_height)  + 1"
   ]
